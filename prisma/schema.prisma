generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id  String  @id @default(uuid())
  username String 
  password String
}


model BpjsKecamatan {
  id          Int     @id @default(autoincrement())
  tahun       Int

  kecamatan   String

  kelasI      Int
  kelasII     Int
  kelasIII    Int
  jumlah      Int

  @@unique([tahun, kecamatan])
  @@map("bpjs_kecamatan")
}

model BpjsKelompokKecamatan {
  id        Int    @id @default(autoincrement())
  tahun     Int

  kecamatan String

  pbi       Int
  nonPbi    Int
  jumlah    Int

  @@unique([tahun, kecamatan])
  @@map("bpjs_kelompok_kecamatan")
}

